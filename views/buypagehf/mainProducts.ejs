        $('.quantity-plus').click(function(e){
          e.preventDefault();
            console.log($('#purchaseNumber').val());
    
            $('#purchaseNumber').val( function(i, oldval) {
                return ++oldval;
            });
    
        });
    
        $('.quantity-minus').click(function(e){
          e.preventDefault();
            console.log($('#purchaseNumber').val());
    
            $('#purchaseNumber').val( function(i, oldval) {
                if(oldval <= 1) {
                    return oldval;
                }
                return --oldval;
            });
    
         });
    </script>
