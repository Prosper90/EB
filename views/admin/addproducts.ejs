<%- include ("header.ejs"); %>
<div class="page-header">
    <div class="row">
    <div class="col">
      <h3 class="page-title">Add new Products</h3>
      <div class="">This page adds products to existing products</div>
    </div>
    </div>
</div>


<div class="row">
   <div class="col-md-12">
    <div class="card">
    <div class="card-header">
    <h4 class="card-title">Product info</h4>
    </div>
    <div class="card-body">
    <form method="post" action="/addproducts">
    <div class="form-group">
    <label>Name</label>
    <input type="text" name="name" class="form-control">
    </div>
    <div class="form-group">
    <label>type <small>type of media</small> </label>
    <input type="text" name="type" class="form-control">
    </div>
    <div class="form-group">
    <label>Products's password</label>
    <input type="text" name="password" class="form-control">
    </div>
    <div class="form-group">
    <label>price </label>
    <input type="number" name="price" class="form-control">
    </div>
    <div class="form-group">
    <label>description</label>
    <input type="text" name="description" class="form-control">
    </div>
    <div class="text-end">
    <button type="submit" class="btn btn-primary">Submit</button>
    </div>
    </form>
    </div>
    </div>
    </div>
</div>
<%- include ("footer.ejs"); %>