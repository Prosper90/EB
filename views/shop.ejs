<%- include ("header.ejs"); %>
<!-- End Header -->
<style>
  .table-container {
    background-color: #f8f8f8;
    border-radius: 7px;
    margin-top: 10px;
    padding: 10px;
    width: 100%;
  }

  @media (min-width: 768px) {
    .table-container {
      overflow-x: visible;
    }
  }

  .order-table {
    width: 100%;
    min-width: 600px;
  }

  .order-table tbody {
    border-bottom: 1px solid #e0dede;
}


.order-table tbody td {
    padding: 2.1rem 1rem;
    border-top: 1px solid #e0dede;
}

.order-table thead th {
    padding: 0.7rem 1rem;
    color: #333;
    font-weight: 300;
    text-align: left;
}

tbody td {
    padding: 14px !important;
    font-weight: 500;
}
  .table_sell {
    background-color: #54524d;
    border-radius: 20px !important;
    color: #fff !important;
  }

  .table_sell th {
    color: #fff !important;
    font-size: 16px !important;
  }

  tbody td {
    padding: 14px !important;
    font-weight: 500;
  }

  .first-cell-container {
    display: flex;
    gap: 5px;
  }

  .first-cell-container .img {
    background-color: #ffa945;
  }

  .btn-dim {
    color: #fff;
    border-color: #54524d;
    background-color: #54524d;
}
</style>

<div class="hero-one" data-background="assets/img/hero/hero-bg-1.png" style="height: 100%;" >


  <div class="p-4 d-flex flex-column gap-5">

      <!--facebook shop-->
      <div class="">
        <div
          class="btn btn-dim btn-block mt-4"
          style="border-radius: 7px; text-align: start; padding-left: 5px"
        >
          FaceBook
        </div>
        <div class="table-container">
          <table class="order-table">
            <thead>
              <tr class="table_sell">
                <th>Title</th>
                <th></th>
                <th>Price</th>
                <th>Stock</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <% let faceonetype; %>
              <%faceBook?.map((data, idx) => {%>
              <tr>
                <% faceonetype = data.type %>
                <td class="order-number" style="white-space: nowrap">
                  <div class="first-cell-container">
                    <div class="img">
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-facebook"><path d="M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z"></path></svg>
                    </div>
                    <div class=""><%= data.title %></div>
                  </div>
                </td>
                <td></td>
                <td class="order-date">
                  <span>NGN<%= data.price.toLocaleString() %></span>
                </td>
                <td class="order-status">
                  <span> <%= data.numOfItem %> pcs.</span>
                </td>
                <% if(data.numOfItem === 0) {%>
                <td class="order-total"><span>Out of stock</span></td>
                <%} else {%>
                <td class="order-action">
                  <a
                    href="/buypage/<%=data._id%>"
                    class="btn btn-secondary btn-outline btn-block btn-rounded btn-sm"
                    >purchase</a
                  >
                </td>
                <%}%>
              </tr>
              <% }) %>
            </tbody>
          </table>
        </div>

        <a
          href="/moretable/<%=faceonetype%>"
          class="btn btn-block btn-primary mt-6 w-100 mt-2"
          style="border-radius: 7px; text-align: center"
        >
          View All
        </a>
      </div>
