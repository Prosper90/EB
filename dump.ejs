<div class="row product-wrapper cols-lg-12 cols-md-4 cols-sm-3 cols-2">
  <div class="product-wrap">
    <div class="product shadow-media text-center">
      <figure class="product-media">
        <a>
          <img
            src="images/products/facebook.png"
            alt="product"
            width="295"
            height="369"
          />
        </a>
      </figure>
      <div class="product-details">
        <h5 class="product-name">
          <a> Facebook </a>
        </h5>
        <div class="">
          <label>Choose type</label>
          <select id="ddlViewBy">
            <%let resData = products.filter((item, index) => { %> <% return
            index === products.findIndex((obj) => { %> <% return item.type ===
            obj.type %> <% }) %> <% }); %> <%resData.map((data, index) => {%>
            <%if(data.name == 'facebook') {%>
            <option value="<%=data.type%>"><%=data.type%></option>
            <%}%> <%})%>
          </select>
        </div>
        <a id="purchasefacebook" class="btn btn-dim btn-filter mt-3"
          >Purchase</a
        >
      </div>
    </div>
    <!-- End product -->
  </div>

